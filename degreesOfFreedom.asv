function [ d1p, d1c, d2p, d2c ] = degreesOfFreedom( M, N, alpha, beta )
%DEGREESOFFREEDOM Summary of this function goes here
%   Detailed explanation goes here

limit = max([M N]);

d1p = 0:0.1:limit;
d1c = 0:0.1:limit;
d2p = 0:0.1:limit;
d2c = 0:0.1:limit;

c1 = d1p(d1p<=f(N(1),beta(1,2),(min([M(1) N(2)])),alpha(1,1),pos((M(1)-N(2)))));
d1c = d1c(d1c<= min([N(1) M(1) N(2)]));
d2c = d2c(d2c<= (min([N(1) M(2)])*alpha(2,1)/alpha(2,2)));
d1p2 = d1p(d1p<= min([M(1) N(1)]) - max(d1c));

end

